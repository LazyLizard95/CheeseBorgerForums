<ol class="post-list">
  {{#each categories as |category| }}
  <li>
    <article class="post">
      <div class="title">
        <a href="/posts/{{category}}/"><span><img src="./images/general.png"width="40" height="40">{{category}}</img></span></a>

      </div>

    </article>
  </li>
  {{/each}}
</ol>